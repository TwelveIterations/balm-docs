package <%= it.package %>.client;

import com.mojang.blaze3d.platform.InputConstants;
import net.blay09.mods.kuma.api.InputBinding;
import net.blay09.mods.kuma.api.Kuma;
import net.blay09.mods.kuma.api.ManagedKeyMapping;
import <%= it.package %>.<%= it.mainClass %>;

import static <%= it.package %>.<%= it.mainClass %>.id;

public class ModKeyMappings {

    public static ManagedKeyMapping yourKey;

    public static void initialize() {
        yourKey = Kuma.createKeyMapping(id("your_key"))
                .withDefault(InputBinding.key(InputConstants.KEY_B))
                .handleScreenInput(event -> {
                    <%= it.mainClass %>.logger.info("B was pressed - " + <%= it.mainClass %>.MOD_ID);
                    return true;
                })
                .build();
    }
}
